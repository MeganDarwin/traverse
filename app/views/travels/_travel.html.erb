<div id="<%= dom_id travel %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= travel.name %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Start date:</strong>
    <%= travel.start_date %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">End date:</strong>
    <%= travel.end_date %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Favorite:</strong>
    <%= travel.favorite %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= travel.user_id %>
  </p>
</div>
